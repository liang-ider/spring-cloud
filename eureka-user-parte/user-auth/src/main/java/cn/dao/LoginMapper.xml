<!DOCTYPE mapper
        PUBLIC "-//MyBatis-dtd"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dao.LoginMapper">
    <select id="loginUser" resultType="cn.entity.User">
        select * from users where user_code=#{user_code}
    </select>
</mapper>

